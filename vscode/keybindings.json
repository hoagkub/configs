// Place your key bindings in this file to override the defaults
[
    {
        "key": "ctrl+shift+t",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "${file}"
        },
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+shift+d",
        "command": "workbench.action.terminal.sendSequence",
        "args": {
            "text": "${fileDirname}/"
        },
        "when": "terminalFocus"
    },
    {
        "key": "ctrl+down",
        "command": "workbench.action.terminal.focus",
    },
    {
        "key": "ctrl+up",
        "command": "workbench.action.focusActiveEditorGroup",
        "when": "terminalFocus"
    }
]